  },
  "Indexer": {
    "StartSlot": 0,
    "BackfillBatch": 100
  },
  "Refund": {
    "CheckPeriodSeconds": 15,
    "BatchSize": 50
  },
  "Commission": {
    "CommissionBps": 100
  }
